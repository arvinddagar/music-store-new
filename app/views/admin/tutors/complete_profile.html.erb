  <%= form_for [:admin, @tutor] , url: complete_profile_path, :html => { :id => "tutor_profile_form" } do |f|%>
    <h3 style="margin-left:300px"> Complete Your Profile </h3>

  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label">Description</label>
    <div class="col-sm-10">
      <%= f.text_area :description,:class=>"textarea_1",:rows=>3 %>
    </div>
  </div>
  <br/>
  <br/>
  <br/>
  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label">About me</label>
    <div class="col-sm-10">
      <%= f.text_area :about_me ,:class=>"textarea_1",:rows=>3%>
    </div>
  </div>
  <br/>
  <br/>
  <br/>
  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label">Duration</label>
    <div class="col-sm-10">
      <%= f.text_field :duration ,:class=>"textbox_1", :style=>"width:50%; margin-bottom:7px;"%>
    </div>
  </div>
  <br/>
  <br/>
  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label">Skills</label>
    <div class="col-sm-10">
      <%= f.text_area :skills,:class=>"textarea_1",:rows=>3 %>
    </div>
  </div>
  <br/>
  <br/>
  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label">Teaching philosophy</label>
    <div class="col-sm-10">
      <%= f.text_area :teaching_philosophy,:class=>"textarea_1",:rows=>3 %>
    </div>
  </div>
  <br/>
  <br/>
  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label">Cre</label>
    <div class="col-sm-10">
      <%= f.text_area :cre ,:class=>"textarea_1",:rows=>3%>
    </div>
  </div>
  <br/>
  <br/>
  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label">Age groups</label>
    <div class="col-sm-10">
      <%= f.text_field :age_group,:class=>"textbox_1", :style=>"width:50%; margin-bottom:7px;" %>
    </div>
  </div>
  <br/>
  <br/>
  <br/>
  <br/>
  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label">Instruments</label>
    <div class="col-sm-10">
      <%= f.text_field :instruments,:class=>"textbox_1", :style=>"width:50%; margin-bottom:7px;" %>
    </div>
  </div>
  <br/>
  <br/>
  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label">Experience</label>
    <div class="col-sm-10">
      <%= f.text_field :experience,:class=>"textbox_1", :style=>"width:50%; margin-bottom:20px;" %>
    </div>
  </div>
  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label">Address</label>
    <div class="col-sm-10">
      <%= f.text_field :address,:class=>"textbox_1", :style=>"width:50%; margin-bottom:20px;"%>
    </div>
  </div>

  <br/>
  <br/>
  <br/>

  <%= submit_tag "save", :class=> "btn btn-info button_profile",:id=>params[:id]%>
<% end %>