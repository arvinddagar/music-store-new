<div class="container">
<h2 style="margin-top:100px">Sign in</h2>

<div class="error">
  <div class="log_in">
  </div>
</div>

<%= form_for resource , as: resource_name, url: session_path(resource_name), remote: true do |f|%>
  <div><%= f.label :email %><br /><br/>
  <%= f.email_field :email, autofocus: true,:class=>"textbox_1",  required: true%></div>
  </br>

  <div><%= f.label :password %><br /><br/>
    <%= f.password_field :password, autocomplete: "off",:class=>"textbox_1",  required: true%></div>
</br>
  <div><%= f.submit "Sign in",:class=>"btn btn-info" %></div>
<% end %>

<%= render "devise/shared/links" %>
</div>